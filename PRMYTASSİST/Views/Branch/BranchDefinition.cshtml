
@{
    ViewBag.Title = "Şube Tanımları";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="kt-content  kt-grid__item kt-grid__item--fluid" id="kt_content" style="padding-top: 5%;">
    <form class="kt-form kt-form--label-right" method="post" action="/Branch/UserAddToBranches">
        <div class="kt-portlet kt-portlet--mobile">
            <div class="kt-portlet__head kt-portlet__head-label center">
                <div class="kt-portlet__head-label pt-2 mt-2">
                    <span class="kt-portlet__head-icon">
                        <i class="flaticon-list"></i>
                    </span>
                    <h5 class="kt-subheader__title">
                        Şube Tanımları
                    </h5>
                </div>
            </div>
            <div class="kt-portlet__body pt-2">
                <!--begin: Datatable -->
                <table class="table table-striped- table-bordered table-hover table-checkable" id="kt_table_brachesList">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>Şube Kodu </th>
                            <th>Şube Adı</th>
                            <th>Bölge Adı</th>
                            <th>Oluşturma Tarihi</th>
                            <th>İşlemler</th>

                        </tr>
                    </thead>
                </table>
                <!--end: Datatable -->
            </div>
        </div>
    </form>
</div>

<script src="./assets/vendors/general/jquery/dist/jquery.js" type="text/javascript"></script>
<script>
    $(document).ready(function () {
        getBranchesList();
    });
</script>